{
   "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
   },
   "include": ["src/**/*.ts"],
}